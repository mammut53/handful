{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_homepage}",
    "sources": "${mod_sources}"
  },
  "license": "${mod_license}",
  "icon": "assets/example/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.mammut53.example.Example"
    ]
  },
  "mixins": [
    "example.mixins.json",
    "example-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric": "*",
    "minecraft": ">=1.21",
    "java": ">=21"
  },
  "custom": {
    "loom:injected_interfaces": {}
  },
  "accessWidener" : "example.accesswidener"
}